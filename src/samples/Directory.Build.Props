<Project>
    <!-- See: https://docs.microsoft.com/en-us/visualstudio/msbuild/customize-your-build?view=vs-2019#directorybuildprops-and-directorybuildtargets -->
    <PropertyGroup>
        <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
        <IsPackable>false</IsPackable>
        <SamplesRoot>$(MSBuildThisFileDirectory)</SamplesRoot>
        <IncludeAspNetSampleHelpers>true</IncludeAspNetSampleHelpers>
    </PropertyGroup>

    <Import Project="$(SamplesRoot)AspnetSampleSupport.props" Condition="'$(IncludeAspNetSampleHelpers)' == 'true'" />
</Project>